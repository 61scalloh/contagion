{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build-normal",
			"type": "shell",
			"command": "C:/Server/cs16/cstrike/addons/amxmodx/scripting/amxxpc.exe",
			"runOptions": {
				"instanceLimit": 1
			},
			"args": [
				"${file}",
				"-i\"C:/Server/cs16/cstrike/addons/amxmodx/scripting/include\"",
				"-i\"C:/Server/cs16/cstrike/addons/amxmodx/scripting/contagion/include\"",
				"-o\"C:/Server/cs16/cstrike/addons/amxmodx/scripting/contagion/compiled/${fileBasenameNoExtension}.amxx\"",
				"-D\"C:/Server/cs16/cstrike/addons/amxmodx/scripting/contagion\""
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"isBackground": false,
			"presentation": {
				"reveal": "always",
				"panel": "dedicated",
				"clear": true
			},
			"problemMatcher": {
				"owner": "pawn",
				"fileLocation": [
					"relative",
					"${workspaceRoot}"
				],
				"pattern": {
					"regexp": "^(.*?)\\(([0-9]*)[- 0-9]*\\) \\: (fatal error|error|warning) [0-9]*\\: (.*)$",
					"file": 1,
					"location": 2,
					"severity": 3,
					"message": 4
				}
			}
		}
	],
}